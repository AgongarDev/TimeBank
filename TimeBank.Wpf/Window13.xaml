        <TextBox HorizontalAlignment="Left" Margin="59,212,0,0" Text="id. usuario" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" FontFamily="Noto Sans" FontSize="16" Foreground="#FF79A386" BorderBrush="{x:Null}" Background="{x:Null}"/>
